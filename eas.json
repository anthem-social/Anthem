{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env" : {
        "EXPO_PUBLIC_ANTHEM_API_URL": "https://api.anthem-social.com"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env" : {
        "EXPO_PUBLIC_ANTHEM_API_URL": "https://api.anthem-social.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
